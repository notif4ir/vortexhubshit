Tab2:CreateButton({
	Name = "No Velocity/Knockback",
	Callback = function()
		game["Run Service"].RenderStepped:Connect(function()
    local movedirection = game.Players.LocalPlayer.Character.Humanoid.MoveDirection
    local jump = game.Players.LocalPlayer.Character.Humanoid.Jump
    if movedirection.Magnitude < 0.01 and not jump then
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
		for i,v in game.Players.LocalPlayer.Character:GetDescendants() do
			if v:IsA("BodyVelocity") or v:IsA("LinearVelocity") or v:IsA("BodyPosition") or v:IsA("BodyThrust") then
				v:Destroy()
			end
		end
    end
end)

game["Run Service"].PostSimulation:Connect(function()
    local movedirection = game.Players.LocalPlayer.Character.Humanoid.MoveDirection
    local jump = game.Players.LocalPlayer.Character.Humanoid.Jump
    if movedirection.Magnitude < 0.01 and not jump then
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
		for i,v in game.Players.LocalPlayer.Character:GetDescendants() do
			if v:IsA("BodyVelocity") or v:IsA("LinearVelocity") or v:IsA("BodyPosition") or v:IsA("BodyThrust") then
				v:Destroy()
			end
		end
    end
end)


game["Run Service"].PreRender:Connect(function()
    local movedirection = game.Players.LocalPlayer.Character.Humanoid.MoveDirection
    local jump = game.Players.LocalPlayer.Character.Humanoid.Jump
    if movedirection.Magnitude < 0.01 and not jump then
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
		for i,v in game.Players.LocalPlayer.Character:GetDescendants() do
			if v:IsA("BodyVelocity") or v:IsA("LinearVelocity") or v:IsA("BodyPosition") or v:IsA("BodyThrust") then
				v:Destroy()
			end
		end
    end
end)
	end
})

Tab2:CreateButton({
	Name = "Disable Animations",
	Callback = function()
		game["Run Service"].RenderStepped:Connect(function()
			for i,v in game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks() do
				v:Stop()
			end
		end)
	end
})

Tab2:CreateButton({
	Name = "Auto Block Cancel",
	Callback = function()
game["Run Service"].RenderStepped:Connect(function()
	if game.Players.LocalPlayer.Character then else return end
	for i,v in game.Workspace.Live:GetChildren() do
		if v:FindFirstChild("HumanoidRootPart") then else continue end
		if v:GetAttribute("Blocking") == true then
			local magnitude = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
			if magnitude <= 25 then
				game.Players.LocalPlayer.Character.Humanoid.AutoRotate=false
				PressMouseMiddle()
				local cf = v.HumanoidRootPart.CFrame + v.HumanoidRootPart.CFrame.LookVector*-2.5
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=cf
				else
				game.Players.LocalPlayer.Character.Humanoid.AutoRotate=true
			end
		end
	end
end)
	end
})

